<Application x:Class="RaceOverlay.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:RaceOverlay.App.Converters"
             xmlns:local="clr-namespace:RaceOverlay.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/Theme.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converters:StringToSolidColorBrushConverter x:Key="StringToSolidColorBrushConverter"/>
            <converters:LapTimeConverter x:Key="LapTimeConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
